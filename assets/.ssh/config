Host 172.17.172.123
  HostName 172.17.172.123
  User tsubasa

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_seirei
  IdentitiesOnly yes

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes