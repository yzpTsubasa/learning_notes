@Library("jenkins_library") _

def build = new org.devops.build()

pipeline {
    agent any
    options {
        disableConcurrentBuilds()
        preserveStashes()
    }

    stages {
        stage('build') {
            steps {
                script {
                    print "Test Build 4"
                }
            }
        }
    }
    
    post { 
        failure { 
            script {
                build.sendCommonResult2DingTalk()
            }
        }
    }
}
